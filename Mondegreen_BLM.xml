	<rules>
		<action type="equip" when="resting" set="Resting" />
		<action type="equip" when="idle" set="Standard" />
		<action type="equip" when="engaged" set="Engaged">
			<body lock="t" />
			<hands lock="t" />
			<feet lock="t" />
		</action>
		<if TPGT="25">
			<action type="Disable" Slot="Main|Sub|Ranged" />
		</if>
		<else>
			<action type="Enable" Slot="Main|Sub|Ranged" />
		</else>
		<if advanced='%MP&gt;%MPMax-30 AND "%EquipLRing"="Ether ring"'>
			<action type="equip" when="precast|midcast">
				<lring lock="t" />
			</action>
		</if>
		<if Skill="Elemental*">
			<action type="equip" when="precast" set="Elemental" />
			<action type="equip" when="precast">
				<main>$%SpellElementStaff</main>
				<sub>Raptor Strap +1</sub>
			</action>
		</if>
		<if Skill="Enfeeb*">
			<if type="BlackMagic">
				<action type="equip" when="precast" set="Enfeebling-INT" />
			</if>
			<elseif type="WhiteMagic">
				<action type="equip" when="precast" set="Enfeebling-MND" />
			</elseif>
			<action type="equip" when="precast">
				<main>$%SpellElementStaff</main>
				<sub>Raptor Strap +1</sub>
			</action>
		</if>
		<if Skill="Enhancing*">
			<action type="equip" when="precast" set="Enhancing" />
			<action type="equip" when="precast">
				<main>$%SpellElementStaff</main>
				<sub>Raptor Strap +1</sub>
			</action>
		</if>
		<if Skill="Dark*">
			<action type="equip" when="precast" set="Dark" />
		</if>
		<if Skill="Healing*">
			<action type="equip" when="precast" set="Healing" />
		</if>
		<if Skill="Divine*">
			<action type="equip" when="precast" set="Divine" />
		</if>
		<!--<action type="equip" when="precast">
			<sub>%SpellElement Grip</sub>
		</action>-->
		<if Spell="Sneak" BuffActive="Sneak" advanced='"%SpellTargetName"="%PlayerName"'>
			<action type="command" when="midcast">cancel 71</action>
			<action type="midcastdelay" delay="2" />
		</if>
		<action type="equip" when="aftercast" set="%Status" />
	</rules>
